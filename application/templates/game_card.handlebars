<div class='game-card mdl-card mdl-shadow--2dp'>
  <div class='mdl-card__title mdl-card--expand' style='background: url("assets/game.jpg") center center no-repeat; background-size: cover;'>
  </div>
  <div class='mdl-card__supporting-text'>
    <span class='title'>{{ title }}</span>
    <span class='score'>{{ score }}</span>
  </div>
  {{#each excerpts }}
    <div class='mdl-card__actions mdl-card--border'>
      <span class='excerpt'>"{{{ excerpt }}}"</span>
      <div class='metrics'>
        <span><span class='metric-title'>BM25</span>{{ bm25 }}</span>
        <span><span class='metric-title'>SENT</span>{{ sent }}</span>
      </div>
    </div>
  {{/each }}
</div>

